<AdministrationPage>
    FloatLayout:
        Button:
            text:"set age parameter"
            size_hint:(.3,.3)
            pos_hint: {'center_x':0.85,'center_y':0.7}
            on_release: root.on_restrict_age()
        Button:
            text:"set occupation parameter"
            size_hint:(.3,.3)
            pos_hint: {'center_x':0.85,'center_y':0.4}
            on_release: root.on_restrict_job()
        Button:
            text:"set medical condition parameter"
            size_hint:(.3,.3)
            pos_hint: {'center_x':0.85,'center_y':0.1}
            on_release: root.on_restrict_mc()
        Button:
            text:"logout"
            size_hint:(.3,.3)
            pos_hint: {'center_x':0.1,'center_y':0.9}
            on_release: root.logout()
        Button:
            text:"Show Vaccine Eligibility Parameters"
            size_hint:(.3,.3)
            pos_hint: {'center_x':0.5,'center_y':0.5}
            on_release: root.show_parameters()